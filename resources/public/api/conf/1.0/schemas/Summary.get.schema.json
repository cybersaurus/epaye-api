{
  "description": "An object containing the account summary.",
  "type": "object",
  "properties": {
    "outstandingCharges": {
      "description": "The outstanding charges on account.",
      "type": "object",
      "properties": {
        "amount": {
          "description": "The outstanding charges on account.",
          "type": "number"
        },
        "breakdown": {
          "description": "The breakdown of outstanding charges.",
          "type": "object",
          "properties": {
            "rti": {
              "description": "The outstanding RTI charges on account.",
              "type": "number"
            },
            "nonRti": {
              "description": "The outstanding Non-RTI charges on account.",
              "type": "number"
            }
          },
          "required": [
            "rti",
            "nonRti"
          ]
        }
      },
      "required": [
        "amount",
        "breakdown"
      ]
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "$ref": "Definitions.schema.json#/definitions/link"
        },
        "empRefs": {
          "$ref": "Definitions.schema.json#/definitions/link"
        }
      },
      "required": [
        "self",
        "empRefs"
      ]
    }
  },
  "required": [
    "outstandingCharges",
    "_links"
  ]
}
