{
  "description": "Wrapper object for individual EmpRefs.",
  "type": "object",
  "properties": {
    "empRefs": {
      "type": "array",
      "items": {
        "description": "An array of objects which you can use to access further data.",
        "type": "object",
        "properties": {
          "empRef": {
            "$ref": "Definitions.schema.json#/definitions/empRef"
          },
          "_links": {
            "type": "object",
            "properties": {
              "summary": {
                "description": "Link to account summary endpoint.",
                "$ref": "Definitions.schema.json#/definitions/link"
              }
            },
            "required": [
              "summary"
            ]
          }
        },
        "required": [
          "empRef",
          "_links"
        ]
      }
    },
    "_links": {
      "type": "object",
      "properties": {
        "self": {
          "$ref": "Definitions.schema.json#/definitions/link"
        }
      },
      "required": [
        "self"
      ]
    }
  },
  "required": [
    "empRefs",
    "_links"
  ]
}
